+            } catch(e) {
+                console.error("Error refreshing grid:", e);
+                // Fallback to page reload if pjax reload fails
+                location.reload();
+            }
+        }
+    });
+});
+
+$(document).on('pjax:success', '#clinics-grid-pjax', function() {
+    console.log('Pjax grid reload successful');
+});
+JS;
+$this->registerJs($js);
+?>
